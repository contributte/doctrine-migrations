{
  "name": "nettrine/migrations",
  "description": "Doctrine Migrations for Nette Framework",
  "keywords": ["nette", "doctrine", "migrations"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/nettrine/migrations",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Josef Benjac",
      "homepage": "http://josefbenjac.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "doctrine/migrations": "^2.0.0",
    "nette/di": "~3.0.0",
    "symfony/console": "^4.2.5"
  },
  "require-dev": {
    "doctrine/orm": "^2.6",
    "mockery/mockery": "^1.2.2",
    "ninjify/qa": "^0.9.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11.0",
    "phpstan/phpstan-nette": "^0.11.0",
    "phpstan/phpstan-shim": "^0.11.5",
    "phpstan/phpstan-strict-rules": "^0.11.0",
    "phpunit/phpunit": "^8.1.0"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Migrations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases",
      "Tests\\Fixtures\\": "tests/fixtures",
      "Tests\\Toolkit\\": "tests/toolkit"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev"
    }
  }
}
